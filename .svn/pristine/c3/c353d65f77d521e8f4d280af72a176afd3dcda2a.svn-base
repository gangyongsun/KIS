<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.goldwind.kis.repository.TermTypeBrowseLogRepository">
	<resultMap id="BaseResultMap" type="cn.com.goldwind.kis.entity.TermTypeBrowseLog">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="empNo" property="empNo" jdbcType="VARCHAR" />
		<result column="termType" property="termType" jdbcType="VARCHAR" />
		<result column="browseCounts" property="browseCounts" jdbcType="INTEGER" />
	</resultMap>

	<sql id="Base_Column_List">
		id,empNo,termType,browseCounts
	</sql>

	<sql id="limit_sql">
		<if test="page_sql != null and page_sql != ''">
			${page_sql}
		</if>
	</sql>

	<sql id="where_all">
		<where>
			<if test="empNo != null and empNo !='' ">
				and (
					empNo= #{empNo,jdbcType=VARCHAR}
				)
			</if>
			<if test="termType != null and termType !='' ">
				and (
					termType= #{termType,jdbcType=VARCHAR}
				)
			</if>
		</where>
	</sql>

	<select id="findAll" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from t_termtypebrowse_log
		<include refid="where_all" />
		order by browseCounts desc
		<include refid="limit_sql" />
	</select>

	<select id="findCount" resultMap="BaseResultMap">
		select count(1) from t_termtypebrowse_log
		<include refid="where_all" />
	</select>
</mapper>